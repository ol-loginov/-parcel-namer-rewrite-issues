{
  "name": "parcel-namer-rewrite-5",
  "source": "src/index.html",
  "parcel-namer-rewrite": {
    "chain": "@parcel/namer-default",
    "rules": {
      "(.*).css": "assets-rewrite/css/$1{.hash}.css",
      "(.*).js": "assets-rewrite/js/$1{.hash}.js",
      "(.*).(jpg|png|gif|svg|webp|avif)": "assets/img1/$1.$2"
    }
  },
  "scripts": {
    "build": "parcel build --dist-dir build src/index.html"
  },
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-namer-rewrite": "2.0.0-rc.0"
  }
}
